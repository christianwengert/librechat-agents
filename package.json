    "@smithy/util-utf8": "^2.0.0",
    "dotenv": "^16.4.5",
    "langchain": "^0.2.10"
  },
  "resolutions": {
    "@langchain/core": "0.2.16",
    "@smithy/eventstream-codec": "^2.0.5",
    "@smithy/protocol-http": "^3.0.6",
    "@smithy/signature-v4": "^2.0.10",
    "@smithy/util-utf8": "^2.0.0"
  },
